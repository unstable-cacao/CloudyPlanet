<table class="table table-hover">
    <thead>
        <tr>
            <th scope="col">ID</th>
            <th scope="col">Artist</th>
            <th scope="col">Name</th>
            <th scope="col">Tags</th>
            <th scope="col">Actions</th>
        </tr>
    </thead>
    <tbody>
    {{#each data}}
        <tr>
            <td>{{ID}}</td>
            <td>{{Artist}}</td>
            <td>{{Name}}</td>
            <td>
                <ul>
                {{#each Tags}}
                    <li>{{Name}}</li>
                {{/each}}
                </ul>
            </td>
            <td></td>
        </tr>
    {{/each}}
    </tbody>
</table>